<?xml version="1.0" encoding="UTF-8"?>
<export>
	<featureMerge>
		<all><!-- all中定义的条件用于全局，一旦设置全局，所有子特征项对应的该属性的条件设置无效 -->
			<nation type="string">all</nation>  <!-- 值为all或以逗号隔开的字符串列表 -->
			<pv type="int">all</pv><!-- 值为all或以逗号隔开的数值上下界，第一个为下界，第二个为上界，包含下界不包含上界 -->
			<impr type="int">all</impr>
			<sv type="int">all</sv>
			<click type="int">all</click>
			<ctr1 type="double">all</ctr1>
			<ctr2 type="double">all</ctr2>
			<ad_fill type="double">all</ad_fill>
		</all>
		<time><!-- 该处定义的条件只适用于该类特征类别 -->
			<fv type="string">all</fv><!-- 有2种设置方式，all，用括号包括再用逗号分隔的字符串列表如：('a','b') -->
			<nation type="string">all</nation>
			<pv type="int">all</pv>
			<impr type="int">all</impr>
			<sv type="int">all</sv>
			<click type="int">1,1000000</click>
			<ctr1 type="double">all</ctr1>
			<ctr2 type="double">all</ctr2>
			<ad_fill type="double">all</ad_fill>
		</time>
		<area>
			<fv type="string">all</fv>
			<nation type="string">all</nation>
			<pv type="int">all</pv>
			<impr type="int">all</impr>
			<sv type="int">all</sv>
			<click type="int">1,1000000</click>
			<ctr1 type="double">all</ctr1>
			<ctr2 type="double">all</ctr2>
			<ad_fill type="double">all</ad_fill>
		</area>
		<browser>
			<fv type="string">all</fv>
			<nation type="string">all</nation>
			<pv type="int">all</pv>
			<impr type="int">all</impr>
			<sv type="int">all</sv>
			<click type="int">all</click>
			<ctr1 type="double">all</ctr1>
			<ctr2 type="double">all</ctr2>
			<ad_fill type="double">all</ad_fill>
		</browser>
		<user>
			<fv type="string">all</fv>
			<nation type="string">all</nation>
			<pv type="int">10,100000</pv>
			<impr type="int">all</impr>
			<sv type="int">all</sv>
			<click type="int">all</click>
			<ctr1 type="double">all</ctr1>
			<ctr2 type="double">all</ctr2>
			<ad_fill type="double">all</ad_fill>
		</user>
		<project>
			<fv type="string">all</fv>
			<nation type="string">all</nation>
			<pv type="int">all</pv>
			<impr type="int">all</impr>
			<sv type="int">all</sv>
			<click type="int">1,1000000</click>
			<ctr1 type="double">all</ctr1>
			<ctr2 type="double">all</ctr2>
			<ad_fill type="double">all</ad_fill>
		</project>
		<query>
			<fv type="string">all</fv>
			<nation type="string">all</nation>
			<length type="int" key="length(fv)">3,300</length>
			<pv type="int">all</pv>
			<impr type="int">all</impr>
			<sv type="int">all</sv>
			<click type="int">all</click>
			<ctr1 type="double">all</ctr1>
			<ctr2 type="double">all</ctr2>
			<ad_fill type="double">all</ad_fill>
		</query>
		<query_length>
			<fv type="string_int">3,300</fv><!-- 有三种设置方式，all，用括号包括再用逗号分隔的字符串列表如：('a','b')，逗号分隔的两个数字,适用于数字类型的字段指定该值得上下界如：3,5 -->
			<nation type="string">all</nation>
			<pv type="int">all</pv>
			<impr type="int">all</impr>
			<sv type="int">all</sv>
			<click type="int">all</click>
			<ctr1 type="double">all</ctr1>
			<ctr2 type="double">all</ctr2>
			<ad_fill type="double">all</ad_fill>
		</query_length>
		<query_word_count>
			<fv type="string_int">2,20</fv>
			<nation type="string">all</nation>
			<pv type="int">all</pv>
			<impr type="int">all</impr>
			<sv type="int">all</sv>
			<click type="int">all</click>
			<ctr1 type="double">all</ctr1>
			<ctr2 type="double">all</ctr2>
			<ad_fill type="double">all</ad_fill>
		</query_word_count>
		<keyword>
			<fv type="string">all</fv>
			<nation type="string">all</nation>
			<length type="int" key="length(fv)">3,30</length><!-- length(fv) -->
			<pv type="int">all</pv>
			<impr type="int">all</impr>
			<sv type="int">all</sv>
			<click type="int">all</click>
			<ctr1 type="double">all</ctr1>
			<ctr2 type="double">all</ctr2>
			<ad_fill type="double">all</ad_fill>
		</keyword>
	</featureMerge>

